{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (enbedded)",
            "type": "cortex-debug",
            "preLaunchTask": "Build",
            "cwd": "${workspaceRoot}",
            "request": "launch",
            "executable": "${command:cmake.launchTargetPath}",
            "servertype": "openocd",
            "searchDir": [],
            "breakAfterReset": true,
            "runToEntryPoint": "main",
            "device": "STM32F446ZE",
            "configFiles": [
                "app/modm_test/modm/openocd.cfg"
            ],
            "rtos": "FreeRTOS",
            "svdFile": "${command:vscode-embedded.st.svd}/STM32F446.svd",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 180000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "type": "console",
                        "label": "Print",
                        "port": 0,
                        "showOnStartup": true
                    },
                ]
            }
        },
        {
            "name": "Launch (windows)",
            "request": "launch",
            "type": "cppvsdbg",
            "cwd": "${workspaceRoot}",
            "program": "${command:cmake.launchTargetPath}",
            "internalConsoleOptions": "openOnSessionStart"
        },
    ]
}