{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "${command:cmake.launchTargetPath}",
            "name": "Debug with OpenOCD",
            "preLaunchTask": "Build",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "searchDir": [],
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "device": "STM32F446ZE",
            "configFiles": [
                "interface/stlink-v2-1.cfg",
                "target/stm32f4x.cfg"
            ],
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 8000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    { "type": "console", "label": "ITM", "port": 0 }
                ]
            }
        },
        {
            "name": "Launch",
            "type": "cppdbg",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${command:cmake.launchTargetPath}",
            "MIMode": "gdb",
            "miDebuggerPath": "${command:vscode-embedded.st.gdb}",
            "miDebuggerServerAddress": "localhost:3333",
            "debugServerPath": "${command:vscode-embedded.st.gdbserver}",
            "debugServerArgs": "--stm32cubeprogrammer-path ${command:vscode-embedded.st.cubeprogrammer} --swd --port-number 3333",
            "serverStarted": "Waiting for connection on port .*\\.\\.\\.",
            "stopAtConnect": false,
            "postRemoteConnectCommands": [
                {
                    "text": "load ${command:cmake.launchTargetPath}"
                }
            ],
            "logging": {
                "engineLogging": true
            },
            "preLaunchTask": "Build",
            "svdPath": "${command:vscode-embedded.st.svd}/STM32F446.svd"
        }
    ]
}